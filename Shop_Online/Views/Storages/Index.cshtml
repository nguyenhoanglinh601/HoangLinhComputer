@model IEnumerable<Shop_Online.Models.Storage>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Amount)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.importDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.exportDate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Amount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.importDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.exportDate)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Product_Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Product_Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Product_Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
